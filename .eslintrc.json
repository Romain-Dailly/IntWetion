{
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/require-default-props": [0, { "forbidDefaultForRequired": true }],
    "react/forbid-foreign-prop-types": [0, { "forbidDefaultForRequired": true }],
    "jsx-a11y/click-events-have-key-events":[0, { "forbidDefaultForRequired": true }],
    "react/prop-types":[0, { "forbidDefaultForRequired": true }]
  },
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  }
}
